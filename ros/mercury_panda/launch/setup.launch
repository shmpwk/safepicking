<launch>

  <arg name="PROCESS" default="true" />
  <arg name="DYNAMIC" default="false" />

  <node name="republish_tf_static_for_rosbag"
        pkg="mercury_panda" type="republish_tf_static_for_rosbag.py"
        clear_params="true" if="$(arg PROCESS)">
  </node>

  <include file="$(find mercury_panda)/launch/rs_rgbd.launch">
    <arg name="GUI" value="false" />
    <arg name="PROCESS" value="$(arg PROCESS)" />
  </include>

  <include file="$(find mercury_panda)/launch/morefusion.launch">
    <arg name="GUI" value="true" />
    <arg name="PASSTHROUGH" value="true" />
    <arg name="PROCESS" value="$(arg PROCESS)" />
    <arg name="DYNAMIC" value="$(arg DYNAMIC)" />
  </include>

</launch>
